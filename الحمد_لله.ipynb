        "account = 83967  # أدخل رقم حسابك\n",
        "password = \"NU6-caQp\"  # أدخل كلمة المرور\n",
        "server = \"RoyalInvestment-Server\"  # أدخل اسم السيرفر\n",
        "\n",
        "if not mt5.login(account, password, server):\n",
